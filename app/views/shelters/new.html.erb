<%= form_tag '/shelters', method: :post %>
<%= label_tag "name"%>
<%= text_field_tag :name, 'Enter name here'%></br>
<%= label_tag "address"%>
<%= text_field_tag :address, "Enter address here"%></br>
<%= label_tag "city"%>
<%= text_field_tag :city, "Enter city here"%></br>
<%= label_tag "state"%>
<%= text_field_tag :state, "Enter state abbreviation here"%></br>
<%= label_tag "zip code"%>
<%= text_field_tag :zip, "Enter five digit zip code here"%></br>
<%= submit_tag :submit%>
